{
  "name": "hospitality-dispatch-service",
  "version": "1.0.0",
  "description": "hospitality-dispatch-service",
  "main": "server.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "babel-node server.js --exec babel-node --presets es2015,stage-2",
    "postinstall": "apidoc -i routes/ -o documentation",
    "builddocs": "apidoc -i routes/ -o documentation",
    "test": "cross-env NODE_ENV=test mocha --timeout 15000 --compilers js:babel-core/register ./test "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CognizantStudio/hospitality-dispatch-service.git"
  },
  "author": "Cognizant Technology Solutions",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "https://github.com/CognizantStudio/hospitality-dispatch-service.git#README.md",
  "dependencies": {
    "apidoc": "^0.17.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "mongo-querystring": "^4.1.1",
    "mongodb": "^2.2.30",
    "mongoose": "^4.11.4",
    "mongoose-unique-validator": "^1.0.5",
    "nexmo": "^2.0.2",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "expect": "^1.20.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "mocha": "^3.4.2",
    "prettier": "^1.5.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "engines": {
    "node": "6.11.1"
  }
}
